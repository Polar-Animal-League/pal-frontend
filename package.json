{
  "name": "@pal/frontend",
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && npm run cpwww",
    "cpwww": "rimraf ./dist/www && ncp ./www ./dist/www",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "npm run build && env-cmd cross-conf-env docker-compose up --build app",
    "dev:watch": "env-cmd nodemon --watch src -e ts,json --exec \"npm run dev\"",
    "start": "react-scripts start"
  },
  "author": "Micha_i",
  "repository": {
    "type": "git",
    "url": "git://github.com/Polar-Animal-League/Frontend.git"
  },
  "license": "",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@polar-animal-league/shared": "^0.0.14",
    "@ryanar/react-auth-provider": "^0.3.3",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "chalk": "^4.1.0",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "mollify": "^3.0.1",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.14.25",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "cross-conf-env": "^1.2.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-formatter-github": "^1.0.11",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
