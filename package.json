{
  "name": "@pal/frontend",
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && npm run cpwww",
    "cpwww": "rimraf ./dist/www && ncp ./www ./dist/www",
    "lint": "eslint . --ext .ts",
    "dev": "npm run build && cross-conf-env docker-compose up --build app",
    "dev:watch": "env-cmd nodemon --watch src -e ts,json --exec \"npm run dev\""
  },
  "author": "Micha_i",
  "repository": {
    "type": "git",
    "url": "git://github.com/Polar-Animal-League/Frontend.git"
  },
  "license": "",
  "dependencies": {
    "@polar-animal-league/shared": "0.0.1",
    "colors": "^1.4.0",
    "express": "^4.17.1",
    "mollify": "^3.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "cross-conf-env": "^1.2.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-formatter-github": "^1.0.11",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  }
}
